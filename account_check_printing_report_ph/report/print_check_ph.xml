<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>

    <template id="print_check_ph">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <div t-att-data-oe-model="o._name" t-att-data-oe-id="o.id" t-foreach="o._check_get_pages()" t-as="page" class="page article">
                    <t t-call="account_check_printing_report_ph.layout_print_check_ph" />
                </div>
            </t>
        </t>
    </template>

    <template id="print_check_ph_corp">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <div t-att-data-oe-model="o._name" t-att-data-oe-id="o.id" t-foreach="o._check_get_pages()" t-as="page" class="page article">
                    <t t-call="account_check_printing_report_ph.layout_print_check_ph_corp" />
                </div>
            </t>
        </t>
    </template>


</data>
</odoo>
