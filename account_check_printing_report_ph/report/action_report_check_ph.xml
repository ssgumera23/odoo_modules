<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="action_report_check_base_ph" model="ir.actions.report">
            <field name="name">Payment Check - PH</field>
            <field name="model">account.payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">account_check_printing_report_ph.report_check_ph</field>
            <field name="report_file">account_check_printing_report_ph.report_check_ph</field>
            <field name="print_report_name">'Payment Check - %s' % (object.partner_id.name or '',)</field>
            <field name="paperformat_id" ref="account_check_printing_report_ph.paperformat_check_ph"/>
            <field name="binding_type">report</field>
            <field name="binding_model_id" ref="account.model_account_payment"/>
        </record>

        <record id="action_report_check_base_ph_corp" model="ir.actions.report">
            <field name="name">Payment Check - PH Corp</field>
            <field name="model">account.payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">account_check_printing_report_ph.report_check_ph_corp</field>
            <field name="report_file">account_check_printing_report_ph.report_check_ph_corp</field>
            <field name="print_report_name">'Payment Check - %s' % (object.partner_id.name or '',)</field>
            <field name="paperformat_id" ref="account_check_printing_report_ph.paperformat_check_ph_corp"/>
            <field name="binding_type">report</field>
            <field name="binding_model_id" ref="account.model_account_payment"/>
        </record>

    </data>
</odoo>
